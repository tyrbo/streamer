<div class="col-md-5">
  <div class="row">
    <div class="col-md-9">
      <div class="row">
        <div class="col-md-12 text-right"><strong>{{player.name}}</strong></div>
      </div>
      <div class="row">
        <div class="col-md-12 text-right">{{player.kills}} / {{player.deaths}} / {{player.assists}}&nbsp;&nbsp;<span class="glyphicon glyphicon-stats"></span></div>
      </div>
      <div class="row">
        <div class="col-md-6">{{player.gold}}</div>
        <div class="col-md-6 text-right">{{player.creeps}}&nbsp;&nbsp;<span class="glyphicon glyphicon-screenshot"></span></div>
      </div>
    </div>
    <div class="col-md-3">
      <span {{bind-attr class=player.champion}}></span>
    </div>
  </div>
</div>
